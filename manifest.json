{
    "manifest_version" : 2,
    "name" : "SEO Keyword Analyser",
    "version" : "0.2",
    "content_scripts" : [
        {
	    "js" : ["content.js"],
 	    "matches" : [
	        "<all_urls>"
	    ]	
	}
    ],
    "browser_action": {
        "default_title": "Test Extension"
    },
    "background" : {
        "scripts" : ["background.js"],
        "persistent" : false
    },
    "permissions": [
        "tabs"
    ]
}
